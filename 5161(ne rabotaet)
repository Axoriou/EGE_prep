import sys
import threading
def f(n):
    if n>=10000:
        return n
    if n<10000 and n%6==0:
        return n//6+f(n//6+2)
    if n<10000 and n%6!=0:
        return n+f(n+2)
def main():
    print(f(264)-f(7))
if __name__ == '__main__':
    sys.setrecursionlimit(100000)
    threading.stack_size(200000000)
    thread = threading.Thread(target=main())
    thread.start()
